using System;

namespace for_loop
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int x = 1; x <= 5; x++)
            {
                for (int i = 1; i <= 10; i++)
                {
                    Console.WriteLine(i);
                }
                for (int a = 10; a >= 0; a--)
                {
                    Console.WriteLine(a);
                }
            }
        }
    }
}
