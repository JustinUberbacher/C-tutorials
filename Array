using System;

namespace Array
{
    class Program
    {
        static void Main(string[] args)
        {
            var studentGrades = new int[] { 80, 77, 90, 20, 99, 95, 65, 75, 80, 98, 25, 63, 59};

            foreach (var studentGrade in studentGrades)
            {
                Console.WriteLine(studentGrade);
            }
        }
    }
}
